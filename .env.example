# APP
LOG_LEVEL=info
# allowed values: json, simple
LOG_FORMAT=json
# allowed values: in_memory, redis
FILTER_BACKEND=redis
# ROUTER_PREFIX=/
# If you uncomment this variable, you enable authorization for the /webhook/grafana/ handler.
# ACCOUNTS='{"login": "password"}'

# VAULT
VAULT_ADDR=http://127.0.0.1:8200
VAULT_TOKEN=token
VAULT_SECRET_PATH=secret/data/alert-manager

# SENTRY
SENTRY_DSN=
SENTRY_CA_CERTS=

# REDIS
REDIS_URL=redis://localhost:6379/0

# SLACK
# Features -> OAuth & Permissions -> OAuth Tokens for Your Workspace
SLACK_TOKEN=
# App level token for establishing a connection through Socket Mode
# You can create it when you enable Socket Mode (Settings -> Socket Mode)
SLACK_SOCKET_MODE_TOKEN=
